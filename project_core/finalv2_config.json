{
    "workflow_id": "Etsy_SEO_Pipeline_V2_Full_Demo",
    "steps": [
        {
            "id": "DATA_LOAD_PRODUCT_00",
            "name": "Ürün Verilerini Yükle",
            "module": "data_loader.py",
            "i": {"file_path": "product_data.json"},
            "o": {"context_key": "product_specs"},
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Harici product_data.json dosyasını yükler.",
                "author": "system_architect",
                "version": "1.0",
                "last_updated": "2025-10-03T09:00:00Z"
            }
        },
        {
            "id": "MARKET_INTEL_01",
            "name": "CSV'den Pazar İstihbaratı Toplama",
            "module": "csv_ingestor.py",
            "i": {
                "file_path": "data/competitors.csv",
                "resolved_profile": {"$profile": "CompetitorCsvProfile"}
            },
            "o": {
                "contract": "MarketIntelligenceData",
                "context_key": "market_data"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Harici bir CSV dosyasını, merkezi profilleri kullanarak işler ve pazar verisi çıkarır.",
                "author": "system_architect",
                "version": "1.1",
                "last_updated": "2025-10-03T16:00:00Z"
            }
        },
        {
            "id": "VOC_ANALYSIS_01",
            "name": "Müşterinin Sesi Analizi",
            "module": "voc_analyzer.py",
            "i": {
                "reviews": ["Ürün harika, hızlı kargo!", "Kalitesi mükemmel ama beklediğimden küçük geldi."]
            },
            "o": {
                "contract": "VoiceOfCustomerInsights",
                "context_key": "voc_data"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Müşteri yorumlarını analiz ederek metin yazarlığı için içgörüler üretir (Rapor 2.2).",
                "author": "conversion_team",
                "version": "1.0",
                "last_updated": "2025-10-02T13:00:00Z"
            }
        },
        {
            "id": "VISUAL_ANALYSIS_01",
            "name": "Görsel Uyum Analizi",
            "module": "visual_analyzer.py",
            "i": {
                "image_paths": ["dummy/path/to/image1.jpg"]
            },
            "o": {
                "context_key": "visual_analysis_data"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Görsellerden anahtar kelime uyumunu analiz eder.",
                "author": "creative_team",
                "version": "1.0",
                "last_updated": "2025-10-03T11:00:00Z"
            }
        },
        {
            "id": "KEYWORD_PREP_01",
            "name": "Stratejik Anahtar Kelime Hazırlama (Konsolide)",
            "module": "keyword_processor.py",
            "i": {
                "seed": "gümüş kolye",
                "market_tags": {"$ref": "context.market_data.topCompetitorTags"},
                "visual_tags": {"$ref": "context.visual_analysis_data.visual_tags"}
            },
            "o": {
                "contract": "StrategicKeywordData",
                "context_key": "keyword_data"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Toplama, filtreleme ve birleştirme işlemlerini tek adımda yapar (Rapor 1.1).",
                "author": "system_architect",
                "version": "1.0",
                "last_updated": "2025-10-02T14:00:00Z"
            }
        },
        {
            "id": "TITLE_GEN_01",
            "name": "Davranışsal Başlık Optimizasyonu",
            "module": "title_optimizer.py",
            "i": {
                "primary_keywords": ["El Yapımı Seramik Kupa"],
                "secondary_keywords": ["Minimalist", "Mavi Sır"],
                "intent_keywords": ["Ofis Hediyesi", "Kahve Severler"]
            },
            "o": {
                "context_key": "listing_title"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Mobil CTR'yi maksimize etmek için başlıkları optimize eder (Rapor 2.3.2).",
                "author": "seo_team",
                "version": "1.0",
                "last_updated": "2025-10-02T12:00:00Z"
            }
        },
        {
            "id": "MAB_OPTIMIZATION_01",
            "name": "MAB Başlık Optimizasyonu",
            "module": "mab_optimizer.py",
            "i": {
                "variations": [
                    {"id": "V1", "title": "El Yapımı Seramik Kupa | Hızlı Kargo"},
                    {"id": "V2", "title": "Minimalist Seramik Fincan | Ofis Hediyesi"}
                ]
            },
            "o": {
                "context_key": "mab_results"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Başlık varyasyonlarını MAB algoritması ile test eder ve trafiği optimize eder (Rapor 2.4).",
                "author": "data_science_team",
                "version": "1.0",
                "last_updated": "2025-10-02T15:00:00Z"
            }
        },
        {
            "id": "FEEDBACK_LOOP_01",
            "name": "Geri Besleme Döngüsünü Çalıştır",
            "module": "feedback_processor.py",
            "i": {
                "performance_data": [
                    {"keyword": "gümüş kolye", "sales": 10, "traffic": 100},
                    {"keyword": "minimalist takı", "sales": 0, "traffic": 100},
                    {"keyword": "ofis hediyesi", "sales": 3, "traffic": 100}
                ]
            },
            "o": {
                "context_key": "feedback_results"
            },
            "rs": {"ruleset_name": "AlwaysRun"},
            "meta": {
                "description": "Performans verilerini işleyerek bilgi bankasını günceller.",
                "author": "system_architect",
                "version": "1.0",
                "last_updated": "2025-10-03T14:00:00Z"
            }
        }
    ]
}